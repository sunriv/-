OUTPUT_FORMAT("elf32-littlearm", "elf32-littlearm", "elf32-littlearm")
OUTPUT_ARCH(arm)
ENTRY(_start)
SECTIONS
{
	. = 0x30000000;

	. = ALIGN(4);
	.text :
	{
		hal/smdk2440/entry.o(.text)
		*(.text)
	}

	. = ALIGN(4);
	.data : { *(.data) }

	. = ALIGN(4);
	.bss : { *(.bss) }

	. = ALIGN(4);
	.rodata : { *(.rodata) }

	. = ALIGN(16);
	armboot_end = .;
}
